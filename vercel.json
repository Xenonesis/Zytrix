{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "build/client",
  "devCommand": "remix vite:dev",
  "framework": "remix",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}